{
  "name": "sketch-v3",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build:git": "vite build --mode git --base https://www.eightfeet.cn/sketch-v3",
    "build:mini": "vite build --mode mini --base ./",
    "demo:publish": "gh-pages -m \"publish demo\" -d dist",
    "demo": "npm run build:git && npm run demo:publish"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.33.0",
    "antd-mobile": "^5.32.1",
    "antd-mobile-icons": "^0.3.0",
    "classnames": "^2.3.2",
    "clipboard": "^2.0.11",
    "dayjs": "^1.11.9",
    "howler": "^2.2.4",
    "postcss-px-to-viewport": "^1.1.1",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-audio-player": "^0.17.0",
    "react-countdown-circle-timer": "^3.2.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "valtio": "^1.11.2",
    "wc-waterfall": "^0.0.7"
  },
  "devDependencies": {
    "@types/howler": "^2.2.11",
    "@types/node": "^20.5.7",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "gh-pages": "^6.0.0",
    "sass": "^1.66.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
